Formulario CesionDatosExpediente campo=SolicitudGenerica permiso=solicitudes {
	
	Menu{
		Enlace "Volver" anterior
		Grupo "Solicitud"  {
		      Enlace "Documentos" pagina=DocumentacionFAP
		      Enlace "Cesiones de datos" pagina=CesionDatosExpedienteSVD
		      Enlace "Autorizar Cesión de datos" pagina=CesionDatos
	    }
		
		Enlace "Ayuda" pagina=Soporte crear
		Enlace "Salir" accion=logout	
	}

	Pagina CesionDatosExpedienteSVD titulo="Cesiones de datos" campo=SolicitudGenerica permiso=adminOrGestor noForm{
		
		Grupo gprTipoServicioSVD titulo="Seleccione el tipo de cesión de datos" {
    		Combo tipoServicioSVD titulo="Cesión de datos de: " campo=SolicitudGenerica.nombreServicio requerido
    		
    		Grupo GrupoTablaIdentidad borde=false mostrarSiCombo tipoServicioSVD="identidad" {
    			Tabla solicitudesPeticionIdentidad titulo="Cesiones de datos de identidad" campo=SolicitudTransmisionSVDFAP seleccionable="Enviar Solicitudes Identidad" 
    			paginaLeer=VerCesionesDatosIdentidadExpedienteSVD paginaCrear=EditarCesionesDatosIdentidadExpedienteSVD paginaEditar=EditarCesionesDatosIdentidadExpedienteSVD
    			{
    				Columna campo=SolicitudTransmisionSVDFAP.datosGenericos.transmision.idTransmision titulo="Identificador de cesión" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.idExpediente titulo="Expediente asociado" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaCreacion titulo="Fecha de creación" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaPeticion titulo="Fecha de petición" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaRespuesta titulo="Fecha de respuesta" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.justificanteSVD.enlaceDescarga titulo="Descargar" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.estado titulo="Estado" expandir
    			}
    		}
    		
    		Grupo GrupoTablaResidencia borde=false mostrarSiCombo tipoServicioSVD="residencia"{
    			Tabla solicitudesPeticionResidencia titulo="Cesiones de datos de residencia" campo=SolicitudTransmisionSVDFAP seleccionable="Enviar Solicitudes Residencia" 
    			paginaLeer=VerCesionesDatosIdentidadExpedienteSVD paginaCrear=EditarCesionesDatosResidenciaExpedienteSVD paginaEditar=EditarCesionesDatosResidenciaExpedienteSVD
    			{
    				Columna campo=SolicitudTransmisionSVDFAP.datosGenericos.transmision.idTransmision titulo="Identificador de cesión" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.idExpediente titulo="Expediente asociado" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaCreacion titulo="Fecha de creación" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaPeticion titulo="Fecha de petición" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.fechaRespuesta titulo="Fecha de respuesta" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.justificanteSVD.enlaceDescarga titulo="Descargar" expandir
    				Columna campo=SolicitudTransmisionSVDFAP.estado titulo="Estado" expandir
    			}
    		}
    	}
	}
	
	Pagina EditarCesionesDatosIdentidadExpedienteSVD titulo="Datos de la cesión de identidad" campo=SolicitudTransmisionSVDFAP permiso=adminOrGestor {
		Grupo datosGenericos titulo="Datos genéricos de la Solicitud" {
			Grupo datosSolicitante titulo="Datos del solicitante"{
				Texto titulo="CIF del Organismo Solicitante: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.identificadorSolicitante requerido
			  	Texto titulo="Nombre del Organismo/Organo: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.nombreSolicitante requerido
			  	Texto titulo="Unidad tramitadora: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.unidadTramitadora
			    Texto titulo="Código del Procedimiento: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.procedimiento.codigoProcedimiento
                Texto titulo="Nombre del procedimiento: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.procedimiento.nombreProcedimiento
			  	Texto titulo="Finalidad: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.finalidad requerido
			  	Combo titulo="Consentimiento: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.consentimiento requerido
			    Texto titulo="Nombre completo: " campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.funcionario.nombreCompletoFuncionario
			}
			Grupo datosTitular titulo="Datos del titular"{
			    Texto titulo="Número de identificación: " campo=SolicitudTransmisionSVDFAP.datosGenericos.titular.documentacion requerido
                Combo titulo="Tipo de identificación: " campo=SolicitudTransmisionSVDFAP.datosGenericos.titular.tipoDocumentacion requerido
			}
		}
		
		Grupo datosEspecificos titulo="Datos específicos de la Solicitud" {
			Texto titulo="Tipo de solicitante de datos: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.tipoSolicitante
			Grupo datosResidencia titulo="Residencia" {
				Combo ComboComunidadAutonoma titulo="Comunidad Autónoma: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.residencia.comunidadAutonoma busqueda
				Combo ComboProvincia titulo="Provincia: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.residencia.provincia busqueda dependeDe=ComboComunidadAutonoma
				Combo ComboIsla titulo="Isla: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.residencia.isla busqueda dependeDe=ComboProvincia
				Combo ComboMunicipio titulo="Municipio: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.residencia.municipio busqueda dependeDe=ComboIsla
			}
			Grupo datosNacimiento titulo="Nacimiento" {
				Fecha titulo="Fecha de nacimiento: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.solicitudNacimiento.fecha
				Combo ComboProvinciaNacimiento titulo="Provincia: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.solicitudNacimiento.provincia
				Combo ComboMunicipioNacimiento titulo="Municipio: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.solicitudNacimiento.municipio dependeDe=ComboProvinciaNacimiento
			    Combo ComboTitulo titulo="Español: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.solicitud.espanol
			}

		}
	}
	
	Pagina VerCesionesDatosIdentidadExpedienteSVD titulo="Cesiones de datos individuales" campo=SolicitudTransmisionSVDFAP permiso=adminOrGestor{
	      Grupo gprRespuesta titulo="Respuesta a la petición de datos" permiso=soloLeer{
              Texto titulo="Identificador de la cesión: " campo=SolicitudTransmisionSVDFAP.datosGenericos.transmision.idTransmision
              Texto titulo="Estado: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.estado.literalError
              Fecha titulo="Fecha de respuesta recibida: " campo=SolicitudTransmisionSVDFAP.fechaRespuesta
              Grupo grpDatosIdentidad titulo="Datos de la identidad del ciudadano"  mostrarSiExpresion "(solicitudTransmisionSVDFAP.estado == \"recibida\")" {
                  Texto titulo="Nombre completo: " campo=SolicitudTransmisionSVDFAP.datosGenericos.titular.nombreCompleto
                  Texto titulo="Primer apellido: " campo=SolicitudTransmisionSVDFAP.datosGenericos.titular.apellido1
                  Texto titulo="Segundo apellido: " campo=SolicitudTransmisionSVDFAP.datosGenericos.titular.apellido2
                  Texto titulo="Nacionalidad: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.nacionalidad
                  Texto titulo="Nombre del padre: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.nombrePadre
                  Texto titulo="Nombre de la madre: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.nombreMadre
                  Fecha titulo="Fecha de caducidad: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.fechacaducidad
                  Texto titulo="Sexo: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.sexo.nombre
                  
                  Grupo datosNacimiento titulo="Datos nacimiento" {
                      Fecha titulo="Fecha de nacimiento: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosNacimiento.fecha
                      Combo ComboProvinciaNacimiento titulo="Provincia: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosNacimiento.provincia
                      Combo ComboLocalidadNacimiento titulo="Localidad: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosNacimiento.municipio
                  }
                  
                  Grupo datosDireccion titulo="Datos dirección" {
                      Combo titulo="Provincia: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosDireccion.provincia
                      Texto titulo="Localidad: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosDireccion.localidad
                      Texto titulo="Tipo de vía: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosDireccion.datosVia.tipo
                      Texto titulo="Vía: " campo=SolicitudTransmisionSVDFAP.datosEspecificos.datosEspecificosIdResi.datosTitular.datosDireccion.datosVia.nombre
                  }
              }           
      }
	}
	
	Pagina EditarCesionesDatosResidenciaExpedienteSVD titulo="Solicitud de Transmisión - Residencia" campo=SolicitudTransmisionSVDFAP permiso=adminOrGestor {
		Grupo datosGenericos titulo="Datos genéricos de la Solicitud" {
			Grupo datosSolicitante titulo="Datos del solicitante"{
				Texto titulo="CIF del Solicitante" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.identificadorSolicitante requerido
			  	Texto titulo="Nombre del Solicitante" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.nombreSolicitante
			  	Texto titulo="Finalidad" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.finalidad
			  	Texto titulo="Unidad tramitadora" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.unidadTramitadora
			  	Texto titulo="Código del Procedimiento" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.procedimiento.codigoProcedimiento
			  	Texto titulo="Nombre del procedimiento" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.procedimiento.nombreProcedimiento
			  	Combo titulo="Consentimiento" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.consentimiento
			  	Grupo datosFuncionario titulo="Datos del funcionario" {
			  		Texto titulo="Nombre completo" campo=SolicitudTransmisionSVDFAP.datosGenericos.solicitante.funcionario.nombreCompletoFuncionario
