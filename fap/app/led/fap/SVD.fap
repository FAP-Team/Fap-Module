//CESIONES DE DATOS AS�?NCRONAS
Formulario CesionDatos {
    
    Menu {
        Enlace "Volver" anterior
        Grupo "Cesiones de datos"  {
              Enlace "Crear peticiones" pagina=CesionDatosSVDCrear
              Enlace "Listar peticiones" pagina=CesionDatosSVDListar
        }
        
        Enlace "Ayuda" pagina=Soporte crear
        Enlace "Salir" accion=logout    
    }
    
    Pagina CesionDatosSVDCrear titulo="Crear Peticiones de Cesión de Datos" permiso=adminOrGestor {
       Grupo gprTipoServicioSVD titulo="Seleccione el tipo de Servicio" {
          Combo tipoServicioSVD titulo="Servicio: " campo=ComboNombreServicioSVDFAP.nombreServicio requerido                         
          //Combo filtroExpedientes titulo="Filtrar por: " campo=ComboFiltroExpedientesSVD.filtroExpedientes   
          Grupo GrupoTablaSolicitudesAutorizadasIdentidad borde=false mostrarSiCombo tipoServicioSVD="identidad" {
                Tabla tablaSolicitudesIdentidadAutorizadas titulo="Solicitudes de Identidad Autorizadas" campo=SolicitudGenerica
                 seleccionable="Crear peticion Identidad" paginaLeer=LeerSolicitud {
                    Columna campo=SolicitudGenerica.id titulo="ID" ancho="30"
                    Columna campo=SolicitudGenerica.expedienteAed.idAed titulo="Expediente" ancho="150"
                    Columna campo=SolicitudGenerica.estado titulo="Estado Interno" ancho="80" permiso=noVisibleUsuario
                    Columna campo=SolicitudGenerica.estadoValue titulo="Estado" ancho="80" permiso=noVisibleUsuario
                    Columna campo=SolicitudGenerica.estadoUsuario titulo="Estado Usuario" ancho="80"
                    Columna campo=SolicitudGenerica.solicitante.numeroId titulo="ID Solicitante" expandir
                    Columna campo=SolicitudGenerica.solicitante.nombreCompleto titulo="Nombre Solicitante" expandir   
                 }
           }     
           
          Grupo GrupoTablaSolicitudesAutorizadasResidencia borde=false mostrarSiCombo tipoServicioSVD="residencia" {
                Tabla tablaSolicitudesResidenciaAutorizadas titulo="Solicitudes de Residencia Autorizadas" campo=SolicitudGenerica
                 seleccionable="Crear peticion Residencia" paginaLeer=LeerSolicitud {
                    Columna campo=SolicitudGenerica.id titulo="ID" ancho="30"
                    Columna campo=SolicitudGenerica.expedienteAed.idAed titulo="Expediente" ancho="150"
                    Columna campo=SolicitudGenerica.estado titulo="Estado Interno" ancho="80" permiso=noVisibleUsuario
                    Columna campo=SolicitudGenerica.estadoValue titulo="Estado" ancho="80" permiso=noVisibleUsuario
                    Columna campo=SolicitudGenerica.estadoUsuario titulo="Estado Usuario" ancho="80"
                    Columna campo=SolicitudGenerica.solicitante.numeroId titulo="ID Solicitante" expandir
                    Columna campo=SolicitudGenerica.solicitante.nombreCompleto titulo="Nombre Solicitante" expandir        
                 }
           }       
       }
    }
    
    
    Pagina CesionDatosSVDListar titulo="Peticiones de Cesión de Datos" permiso=adminOrGestor {
        Grupo gprTipoServicioSVD titulo="Seleccione el tipo de Servicio" {
            Combo tipoServicioSVD titulo="Servicio: " campo=ComboNombreServicioSVDFAP.nombreServicio requerido
            
            Grupo GrupoTablaCesionesIdentidad borde=false mostrarSiCombo tipoServicioSVD="identidad" {
                Tabla tablaCesionesIdentidad titulo="Peticiones de Identidad" campo=PeticionSVDFAP
                 seleccionable="Enviar peticiones Identidad" paginaLeer=EditarPeticionSVDFAP paginaEditar=EditarPeticionSVDFAP {
                     Columna campo=PeticionSVDFAP.id titulo="ID" ancho="40"
                     Columna campo=PeticionSVDFAP.estadoPeticion titulo="Estado" expandir
                }      
            }
            
            Grupo GrupoTablaCesionesResidencia borde=false mostrarSiCombo tipoServicioSVD="residencia" {
                Tabla tablaCesionesResidencia titulo="Peticiones de Residencia" campo=PeticionSVDFAP
                 seleccionable="Enviar peticiones Residencia" paginaLeer=EditarPeticionSVDFAP paginaEditar=EditarPeticionSVDFAP  {
                     Columna campo=PeticionSVDFAP.id titulo="ID" ancho="40"
                     Columna campo=PeticionSVDFAP.estadoPeticion titulo="Estado" expandir
                }   
            }   
        }
    }
    
    
    Pagina EditarPeticionSVDFAP titulo="Petición de Cesión de Datos" campo=PeticionSVDFAP permiso=adminOrGestor {
        
        Grupo GrupoPeticionDatos titulo="Datos de la Petición" permiso=soloLeer {
            Texto titulo="ID Petición" campo=PeticionSVDFAP.atributos.idPeticion
            Combo titulo="Estado" campo=PeticionSVDFAP.estadoPeticion
            Texto titulo="Num Elementos" campo=PeticionSVDFAP.atributos.numElementos
        }
        
        Grupo grupoTablaSolicitudesIdentidad mostrarSiExpresion "(peticionSVDFAP.nombreServicio == \"identidad\")" {
            Tabla tablaSolicitudesTransmisionIdentidad titulo="Solicitudes de Transmisión" campo=SolicitudTransmisionSVDFAP {
                Columna campo=SolicitudTransmisionSVDFAP.id titulo="ID" ancho="30"
                Columna campo=SolicitudTransmisionSVDFAP.nombreServicio titulo="Tipo" ancho="100"
