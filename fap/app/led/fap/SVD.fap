			Combo titulo="Tipo documentación" campo=SolicitudTransmisionSVDFAP.respuesta.datosGenericos.titular.tipoDocumentacion
			Texto titulo="Documentación" campo=SolicitudTransmisionSVDFAP.respuesta.datosGenericos.titular.documentacion
		
			Grupo respuestaResidencia titulo="Residencia" {
				Grupo datosDomicilio titulo="Domicilio" {
					Combo titulo="Provincia" campo=SolicitudTransmisionSVDFAP.respuesta.datosEspecificos.domicilio.provincia
					Combo titulo="Municipio" campo=SolicitudTransmisionSVDFAP.respuesta.datosEspecificos.domicilio.municipio
					Texto titulo="Entidad colectiva" campo=SolicitudTransmisionSVDFAP.respuesta.datosEspecificos.domicilio.entColectiva.nombre
